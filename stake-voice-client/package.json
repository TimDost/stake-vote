    "uglify-js": "^3.0.28",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.0",
    "web3": "^0.20.1"
